	Note = {To appear},
	Pdf = {http://rigoletto.seas.ucla.edu/papers/Savchenko.Frazzoli.ACC05.pdf},
	Title = {On the Time Complexity of Conflict-Free Vehicle Routing},
	Year = {2005}}

