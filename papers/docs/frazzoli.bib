@inproceedings{Sharma.Frazzoli.ea:CDC05,
	Abstract = {Gupta and Kumar (2000) studied the scaling of capacity of a wireless network when the nodes in the network are fixed at arbitrary locations and showed that for arbitrary setting of nodes the throughput per node achieved scales as $\Theta(1/\sqrt{n})$ when the average distance between the source and destination of bits is constant. Grossglauser and Tse (2001) showed for a random network model, where the nodes are randomly located in the environment and are mobile, a constant throughput per node is achievable by letting data travel on mobile nodes to avoid using the wireless media. In this paper, we consider a network consisting of n static nodes with constant throughput requirements and m mobile agents, which are constrained not to grow faster than a factor of $n$, such that the average distance between the origin and destination of bits is non-vanishing in the number of nodes. It is shown that for any optimal placement of nodes, arbitrary movement of mobile agents and arbitrary assignment of transmissions, in order to achieve constant throughput per node, the average distance the bits have to be transported on mobile agents is nonvanishing. We show that since the mobile agents have to stay a velocity dependent distance away from each other for safety, the time taken for them to travel a non-vanishing average distance increases with the number of agents. This directly affects the maximum delay incurred by all bits transported in the network and we show that the maximum delay is $\Omega(\sqrt{n})$. We also present a placement of static nodes, assignment of origin destination of bits whose average distance is constant, movement of mobile agents and scheduling scheme for transmissions such that the network achieves constant throughput per node and incurs a maximum delay of $O(\sqrt{n})$. },
	Address = {Seville, Spain},
	Author = {V. Sharma and E. Frazzoli and P.~G. Voulgaris},
	Booktitle = {Proc. of the IEEE Conf. on Decision and Control},
	Date-Added = {2005-03-08 16:42:49 -0800},
	Date-Modified = {2005-03-09 15:06:06 -0800},
	Local-Url = {/www/papers/Sharma.Frazzoli.ea.CDC05.DRAFT.pdf},
	Month = {March},
	Note = {Submitted},
	Pdf = {http://rigoletto.seas.ucla.edu/papers/Sharma.Frazzoli.ea.CDC05.DRAFT.pdf},
	Title = {Delay in Mobility-Assisted Constant-Throughput Wireless Networks},
	Year = {2005}}

